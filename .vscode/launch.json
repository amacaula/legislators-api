    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/server.js",
            "smartStep": true,
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cache_data.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/cache_data.js",
            "smartStep": true,
            "env": {
                "NODE_TLS_REJECT_UNAUTHORIZED": "0",
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "graphql_index.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/graphql_index.js",
            "smartStep": true,
        },
        {
            "type": "node",
            "request": "launch",
            "name": "queries.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/client/queries.js",
            "smartStep": true,
        },
        {
            "type": "node",
            "request": "launch",
            "name": "test_xray.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/utils/test_xray.js",
            "smartStep": true,
            "env": {
                "NODE_TLS_REJECT_UNAUTHORIZED": "0",
            }
        }
    ]
}